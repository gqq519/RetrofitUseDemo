<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="com.feicuiedu.demoretrofit"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".demoAOkHttp.OkHttpActivity"
            android:label="OkHttp Get Example"/>
        <activity
            android:name=".demoBGithub.ContributorsActivity1"
            android:label="Retrofit Get Example"/>
        <activity
            android:name=".demoCGithub.ContributorsActivity2"
            android:label="Get Example with GsonConverterFactory"/>
        <activity
            android:name=".demoDGithub.ContributorsActivity3"
            android:label="Retrofit and Mvp"/>
        <activity
            android:name=".demoETV.TvInfoActivity"
            android:label="Post Formed-encoded Data"/>
    </application>

</manifest>
